<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:TextBox ui:field="messageTextBox"></g:TextBox>
			<g:Button ui:field="sendButton" text="Send"></g:Button>
		</g:HorizontalPanel>
		<g:TabLayoutPanel barHeight="22" barUnit="PX" height="400PX">
			<g:tab>
				<g:header>Help</g:header>
				<g:ScrollPanel height="100%" alwaysShowScrollBars="true">
					<g:Label>Run command above</g:Label>
				</g:ScrollPanel>
			</g:tab>
		</g:TabLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder>
